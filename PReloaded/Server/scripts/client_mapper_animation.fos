            uint len = 0;
            int  pos = findLast(sound, "Front");
            if(pos != -1)
                len = 5;
            else
            {
                pos = findLast(sound, "Back");
                if(pos != -1)
                    len = 4;
                else
                    continue;
            }

			for( ; len > 0; len-- )
                sound[-1] = "";

            if(PlaySound(sound + ".wav"))
                return;
        }
    }
}
#endif
