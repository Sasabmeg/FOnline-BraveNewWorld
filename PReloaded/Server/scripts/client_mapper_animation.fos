// Call before 2d animation playing.
void animation2d_process(bool animateStay, CritterCl& cr, uint anim1, uint anim2, ItemCl@ item)
{
    if(!animateStay)
        PlayAnimSound(cr.CrType, cr.Stat[ST_GENDER], anim1, anim2);
}

