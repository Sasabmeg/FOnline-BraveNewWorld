# default line endings
* text eol=lf

# git
.gitignore     text eol=lf
.gitattributes text eol=lf

# intellisense creator
intellisense*.h text eol=lf

# syntax highlighting for .fos files
*.fos linguist-language=C++

# binary
*.acm  binary
*.ACM  binary
*.dat  binary
*.DAT  binary
*.exe  binary
*.EXE  binary
*.dll  binary
*.DLL  binary
*.fosb binary
*.FOSB binary
*.frm  binary
*.FRM  binary
*.fr0  binary
*.FR0  binary
*.fr1  binary
*.FR1  binary
*.fr2  binary
*.FR2  binary
*.fr3  binary
*.FR3  binary
